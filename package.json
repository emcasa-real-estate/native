    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "camelcase-keys-deep": "^0.1.0",
    "chai": "^4.1.2",
    "email-validator": "^1.1.1",
    "es6-error": "^4.1.1",
    "events": "^3.0.0",
