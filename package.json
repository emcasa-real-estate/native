{
  "name": "EmCasa",
  "version": "1.2.0",
  "private": true,
  "rnpm": {
    "assets": ["./src/assets/fonts"]
  },
  "scripts": {
    "prepare": "patch-package",
    "start": "scripts/env react-native start",
    "android": "scripts/env react-native run-android",
    "ios": "scripts/env react-native run-ios",
    "xcode": "open ios/EmCasa.xcworkspace",
    "devtools": "react-devtools",
    "log-android": "react-native log-android",
    "log-ios": "react-native log-ios",
    "test": "jest --config __test__/jest.config.js",
    "build_e2e": "NODE_ENV=e2e scripts/env detox build -c ios.debug",
    "record": "REPLAY_MODE=record scripts/server/start.js",
    "e2e": "detox test -c ios.debug --platform ios"
  },
  "detox": {
    "specs": "__e2e__",
    "test-runner": "jest",
    "runner-config": "./__e2e__/jest.config.js",
    "configurations": {
      "ios.debug": {
        "binaryPath":
          "ios/build/Build/Products/Debug-iphonesimulator/EmCasa.app",
        "build": "scripts/ios/build.sh",
        "type": "ios.simulator",
        "name": "iPhone 6"
      },
      "android.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "build": "scripts/android/build.sh",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      }
    }
  },
  "dependencies": {
    "@absinthe/socket": "^0.1.10",
    "@absinthe/socket-apollo-link": "^0.1.11",
    "@fortawesome/fontawesome-pro-light": "^5.0.10",
